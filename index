import React, { useState, useEffect, useMemo } from 'react';
import { 
  Sparkles, 
  RefreshCw, 
  Search,
  CheckCircle2,
  X,
  RotateCw,
  Compass,
  ChevronDown,
  Star
} from 'lucide-react';

const apiKey = ""; // Runtime provided

const ZODIAC_SIGNS = [
  "Aries", "Taurus", "Gemini", "Cancer", "Leo", "Virgo", 
  "Libra", "Scorpio", "Sagittarius", "Capricorn", "Aquarius", "Pisces"
];

const TAGLINE_OPTIONS = [
  "Minimalist Wisdom for Conscious Souls",
  "Where Intuition Meets Absolute Clarity",
  "Your Path, Distilled to Its Essence",
  "Pure Insight for the Modern Path",
  "The Quiet Voice of Internal Wisdom",
  "Stripping Back the Noise to Find the Truth",
  "Ancient Symbols, Modern Insights",
  "A Clear Mirror for the Inner Self",
  "Practical Magic for Everyday Living",
  "Strategic Insights from the Arcanum"
];

// Complete 78-Card Deck Data
const TAROT_DECK = [
  { name: "The Fool", suit: "Major", element: "Air" }, { name: "The Magician", suit: "Major", element: "Air" },
  { name: "The High Priestess", suit: "Major", element: "Water" }, { name: "The Empress", suit: "Major", element: "Earth" },
  { name: "The Emperor", suit: "Major", element: "Fire" }, { name: "The Hierophant", suit: "Major", element: "Earth" },
  { name: "The Lovers", suit: "Major", element: "Air" }, { name: "The Chariot", suit: "Major", element: "Water" },
  { name: "Strength", suit: "Major", element: "Fire" }, { name: "The Hermit", suit: "Major", element: "Earth" },
  { name: "Wheel of Fortune", suit: "Major", element: "Fire" }, { name: "Justice", suit: "Major", element: "Air" },
  { name: "The Hanged Man", suit: "Major", element: "Water" }, { name: "Death", suit: "Major", element: "Water" },
  { name: "Temperance", suit: "Major", element: "Fire" }, { name: "The Devil", suit: "Major", element: "Earth" },
  { name: "The Tower", suit: "Major", element: "Fire" }, { name: "The Star", suit: "Major", element: "Air" },
  { name: "The Moon", suit: "Major", element: "Water" }, { name: "The Sun", suit: "Major", element: "Fire" },
  { name: "Judgement", suit: "Major", element: "Fire" }, { name: "The World", suit: "Major", element: "Earth" },
  ...['Ace', 'Two', 'Three', 'Four', 'Five', 'Six', 'Seven', 'Eight', 'Nine', 'Ten', 'Page', 'Knight', 'Queen', 'King'].flatMap(rank => 
    ['Cups', 'Pentacles', 'Swords', 'Wands'].map(suit => ({
      name: `${rank} of ${suit}`,
      suit: suit,
      element: suit === 'Cups' ? 'Water' : suit === 'Pentacles' ? 'Earth' : suit === 'Swords' ? 'Air' : 'Fire'
    }))
  )
];

// Custom SVGs mimicking Chloe Zarka Grinsnir's Minimalist Line Art
const EssentialIcon = ({ type }) => {
  const strokeColor = "currentColor";
  switch (type) {
    case 'Major':
      return (
        <svg viewBox="0 0 24 24" fill="none" stroke={strokeColor} strokeWidth="0.8" className="w-12 h-12 opacity-40">
          <circle cx="12" cy="12" r="9" />
          <path d="M12 3v18M3 12h18M18.36 5.64l-12.72 12.72M5.64 5.64l12.72 12.72" />
          <circle cx="12" cy="12" r="2" fill={strokeColor} />
        </svg>
      );
    case 'Cups':
      return (
        <svg viewBox="0 0 24 24" fill="none" stroke={strokeColor} strokeWidth="0.8" className="w-12 h-12 text-blue-400">
          <path d="M7 5h10M8 5v6c0 2.2 1.8 4 4 4s4-1.8 4-4V5M12 15v4M9 20h6" />
          <circle cx="12" cy="8" r="1" fill={strokeColor} />
        </svg>
      );
    case 'Pentacles':
      return (
        <svg viewBox="0 0 24 24" fill="none" stroke={strokeColor} strokeWidth="0.8" className="w-12 h-12 text-amber-600">
          <circle cx="12" cy="12" r="8" />
          <path d="M12 4v16M4 12h16M12 12l5.66-5.66M6.34 17.66l5.66-5.66M12 12l-5.66-5.66M17.66 17.66l-5.66-5.66" />
          <circle cx="12" cy="12" r="1.5" fill={strokeColor} />
        </svg>
      );
    case 'Swords':
      return (
        <svg viewBox="0 0 24 24" fill="none" stroke={strokeColor} strokeWidth="0.8" className="w-12 h-12 text-slate-400">
          <path d="M12 3v14M9 17h6M12 17v4M11 21h2" />
          <path d="M12 3l-2 4h4l-2-4z" fill={strokeColor} />
          <circle cx="12" cy="10" r="0.5" fill={strokeColor} />
        </svg>
      );
    case 'Wands':
      return (
        <svg viewBox="0 0 24 24" fill="none" stroke={strokeColor} strokeWidth="0.8" className="w-12 h-12 text-amber-800">
          <path d="M8 21l8-18M11 17l2-1M9 13l2-1M7 9l2-1" />
          <circle cx="14.5" cy="5.5" r="1" fill={strokeColor} />
        </svg>
      );
    default: return null;
  }
};

const App = () => {
  const [userZodiac, setUserZodiac] = useState('Scorpio');
  const [selectionMode, setSelectionMode] = useState('manual');
  const [drawnCards, setDrawnCards] = useState([]);
  const [reading, setReading] = useState("");
  const [activeTagline, setActiveTagline] = useState("");
  const [isLoading, setIsLoading] = useState(false);
  const [error, setError] = useState(null);
  const [manualSelection, setManualSelection] = useState([null, null, null]);
  const [searchQuery, setSearchQuery] = useState("");
  const [isPickerOpen, setIsPickerOpen] = useState(false);
  const [activePickerSlot, setActivePickerSlot] = useState(null);

  const groupedDeck = useMemo(() => {
    const search = searchQuery.toLowerCase();
    const filtered = TAROT_DECK.filter(card => 
      card.name.toLowerCase().includes(search) ||
      card.suit.toLowerCase().includes(search)
    );
    return {
      "Major Arcana": filtered.filter(c => c.suit === "Major"),
      "Cups": filtered.filter(c => c.suit === "Cups"),
      "Pentacles": filtered.filter(c => c.suit === "Pentacles"),
      "Swords": filtered.filter(c => c.suit === "Swords"),
      "Wands": filtered.filter(c => c.suit === "Wands"),
    };
  }, [searchQuery]);

  const callGemini = async (prompt, retries = 5) => {
    const url = `https://generativelanguage.googleapis.com/v1beta/models/gemini-2.5-flash-preview-09-2025:generateContent?key=${apiKey}`;
    for (let i = 0; i < retries; i++) {
      try {
        const response = await fetch(url, {
          method: 'POST',
          headers: { 'Content-Type': 'application/json' },
          body: JSON.stringify({
            contents: [{ parts: [{ text: prompt }] }],
            systemInstruction: { parts: [{ text: "You are a specialized Tarot reader for The Essential Tarot by Chloe Zarka Grinsnir. Poetic, minimalist, and psychologically insightful. Always end your response with TAGLINE: [selected phrase]." }] }
          })
        });
        if (!response.ok) throw new Error("API_ERROR");
        const data = await response.json();
        return data.candidates?.[0]?.content?.parts?.[0]?.text;
      } catch (err) {
        if (i === retries - 1) throw err;
        await new Promise(r => setTimeout(r, 1000 * Math.pow(2, i)));
      }
    }
  };

  const generateReading = async (cards) => {
    setIsLoading(true);
    setReading("");
    setError(null);
    const cardDetails = cards.map(c => `${c.name} (${c.isReversed ? 'Reversed' : 'Upright'})`).join(', ');
    const prompt = `Querent Sign: ${userZodiac}. Deck: Essential Tarot (Chloe Zarka Grinsnir). Cards: ${cardDetails}. 
    Provide a professional 3-paragraph reading.
    1. Minimalist symbolic interpretation. 2. The 'Essential' psychological core. 3. Practical advice for ${userZodiac}.
    Choose ONE tagline for the very end from this list: [${TAGLINE_OPTIONS.join(', ')}].
    End with: TAGLINE: [Chosen Phrase]`;

    try {
      const result = await callGemini(prompt);
      const tagIndex = result.toUpperCase().lastIndexOf("TAGLINE:");
      if (tagIndex !== -1) {
        setReading(result.substring(0, tagIndex).trim());
        setActiveTagline(result.substring(tagIndex + 8).trim());
      } else {
        setReading(result.trim());
        setActiveTagline(TAGLINE_OPTIONS[Math.floor(Math.random() * TAGLINE_OPTIONS.length)]);
      }
    } catch (err) {
      setError("The cosmic connection flickered. Please try again.");
    } finally {
      setIsLoading(false);
    }
  };

  const handleRandomDraw = () => {
    setIsLoading(true);
    setDrawnCards([]);
    setReading("");
    const shuffled = [...TAROT_DECK].sort(() => 0.5 - Math.random());
    const selected = shuffled.slice(0, 3).map(card => ({
      ...card,
      isReversed: Math.random() > 0.5,
      id: Math.random().toString(36).substr(2, 9)
    }));
    setDrawnCards(selected);
    generateReading(selected);
  };

  const handleManualReading = () => {
    if (manualSelection.includes(null)) return;
    setDrawnCards(manualSelection);
    generateReading(manualSelection);
  };

  const flipCard = (idx) => {
    const next = [...manualSelection];
    if (next[idx]) {
      next[idx] = { ...next[idx], isReversed: !next[idx].isReversed };
      setManualSelection(next);
      if (drawnCards.length > 0) setDrawnCards(next);
    }
  };

  const openPicker = (idx) => {
    setActivePickerSlot(idx);
    setIsPickerOpen(true);
    setSearchQuery("");
  };

  const selectCardFromLibrary = (card) => {
    if (manualSelection.some((c, idx) => idx !== activePickerSlot && c?.name === card.name)) {
      return;
    }
    const next = [...manualSelection];
    next[activePickerSlot] = { 
      ...card, 
      isReversed: false, 
      id: Math.random().toString(36).substr(2, 9) 
    };
    setManualSelection(next);
    setIsPickerOpen(false);
  };

  return (
    <div className="min-h-screen bg-[#fcfcf7] text-zinc-900 font-serif pb-24 touch-pan-y selection:bg-amber-100 overflow-x-hidden">
      <main className="max-w-md mx-auto px-5 pt-8 text-center relative">
        
        {/* Header - Zarka Grinsnir Aesthetic */}
        <header className="mb-10 pt-4 relative">
          <div className="flex justify-center mb-3">
             <div className="w-12 h-[1px] bg-zinc-200 mt-2 mr-3" />
             <div className="w-1 h-1 rounded-full bg-amber-500 mt-1.5" />
             <div className="w-12 h-[1px] bg-zinc-200 mt-2 ml-3" />
          </div>
          <h1 className="text-4xl sm:text-5xl font-extralight tracking-[0.5em] text-zinc-900 mb-2 iPhoneTitle">
            ESSENTIAL
          </h1>
          <p className="text-[9px] tracking-[0.4em] text-zinc-400 uppercase font-sans font-bold mb-8">Chloe Zarka Grinsnir</p>
          
          <div className="inline-flex items-center gap-3 px-6 py-2 bg-white border border-zinc-100 rounded-full shadow-sm">
            <span className="text-[9px] uppercase font-sans font-bold text-zinc-400 tracking-wider">Sign:</span>
            <div className="relative flex items-center">
              <select 
                value={userZodiac} 
                onChange={(e) => setUserZodiac(e.target.value)}
                className="appearance-none bg-transparent pr-6 text-xs font-bold uppercase outline-none cursor-pointer text-zinc-800 font-sans tracking-widest"
              >
                {ZODIAC_SIGNS.map(sign => <option key={sign} value={sign}>{sign}</option>)}
              </select>
              <ChevronDown className="absolute right-0 w-3.5 h-3.5 text-amber-500 pointer-events-none" />
            </div>
          </div>
        </header>

        {/* Tab Controls */}
        <nav className="flex justify-center mb-12">
          <div className="bg-zinc-100 p-1 rounded-full flex w-full max-w-[260px] border border-zinc-200/50 shadow-inner">
            <button 
              onClick={() => { setSelectionMode('random'); setReading(""); setDrawnCards([]); }}
              className={`flex-1 py-2.5 rounded-full text-[10px] uppercase font-bold tracking-widest transition-all duration-500 ${selectionMode === 'random' ? 'bg-zinc-900 text-white shadow-lg' : 'text-zinc-500'}`}
            >
              Random
            </button>
            <button 
              onClick={() => { setSelectionMode('manual'); setReading(""); setDrawnCards([]); }}
              className={`flex-1 py-2.5 rounded-full text-[10px] uppercase font-bold tracking-widest transition-all duration-500 ${selectionMode === 'manual' ? 'bg-zinc-900 text-white shadow-lg' : 'text-zinc-500'}`}
            >
              Manual
            </button>
          </div>
        </nav>

        {/* Board */}
        <section className="mb-14 px-2">
          {selectionMode === 'random' ? (
            <button 
              onClick={handleRandomDraw}
              disabled={isLoading}
              className="group relative px-14 py-5 bg-white border border-zinc-200 rounded-full flex items-center gap-4 shadow-xl active:scale-95 transition-all mx-auto"
            >
              <Compass className={`w-5 h-5 text-amber-500 ${isLoading ? 'animate-spin' : ''}`} />
              <span className="text-[11px] uppercase font-bold tracking-[0.3em] font-sans text-zinc-800">Invoke Oracle</span>
            </button>
          ) : (
            <div className="space-y-10">
              <div className="flex justify-center gap-4">
                {[0, 1, 2].map(idx => {
                  const card = manualSelection[idx];
                  return (
                    <div key={idx} className="relative">
                      <button 
                        onClick={() => openPicker(idx)}
                        className={`w-[104px] h-[165px] sm:w-[120px] sm:h-[185px] rounded-2xl border transition-all duration-500 flex flex-col items-center justify-center bg-white relative ${card ? 'border-zinc-800 shadow-2xl scale-[1.03]' : 'border-dashed border-zinc-300 bg-zinc-50/30'}`}
                      >
                        {card ? (
                          <div className={`p-4 h-full w-full flex flex-col items-center justify-between text-center transition-transform duration-300 ${card.isReversed ? 'rotate-180' : ''}`}>
                             <div className="opacity-60 transform scale-75 mt-4">
                               <EssentialIcon type={card.suit === "Major" ? "Major" : card.suit} />
                             </div>
                             <span className="text-[9px] font-bold text-zinc-900 leading-tight uppercase font-sans tracking-tighter mb-4">{card.name}</span>
                          </div>
                        ) : (
                          <div className="opacity-10 flex flex-col items-center">
                            <Star size={20} className="mb-3" />
                            <span className="text-[8px] uppercase tracking-widest font-sans font-bold">Slot {idx+1}</span>
                          </div>
                        )}
                        <div className="absolute inset-[3px] border border-zinc-50 rounded-[14px] pointer-events-none opacity-40" />
                      </button>
                      {card && (
                        <div className="absolute -top-3 -right-3 flex flex-col gap-2 z-20">
                          <button onClick={(e) => { e.stopPropagation(); flipCard(idx); }} className="p-2.5 bg-white rounded-full border border-zinc-100 shadow-lg text-amber-500 active:scale-90 transition-all"><RotateCw size={14}/></button>
                          <button onClick={(e) => { e.stopPropagation(); const n = [...manualSelection]; n[idx] = null; setManualSelection(n); setDrawnCards([]); }} className="p-2.5 bg-white rounded-full border border-zinc-100 shadow-lg text-red-400 active:scale-90 transition-all"><X size={14}/></button>
                        </div>
                      )}
                    </div>
                  );
                })}
              </div>
              <button 
                onClick={handleManualReading}
                disabled={manualSelection.includes(null) || isLoading}
                className={`px-16 py-4 rounded-full font-bold uppercase text-[10px] tracking-[0.4em] shadow-xl transition-all font-sans ${!manualSelection.includes(null) ? 'bg-zinc-900 text-white hover:scale-105 active:scale-95' : 'bg-zinc-100 text-zinc-300'}`}
              >
                Mirror Spread
              </button>
            </div>
          )}
        </section>

        {/* Drawn Cards Display */}
        {drawnCards.length > 0 && !isLoading && (
          <div className="grid grid-cols-3 gap-4 mb-16 animate-fade-in px-2">
            {drawnCards.map((card, i) => (
              <div key={card.id || i} className={`aspect-[5/8] bg-white rounded-xl border border-zinc-100 shadow-lg overflow-hidden ${card.isReversed ? 'rotate-180' : ''}`}>
                <div className="h-full w-full p-4 flex flex-col items-center justify-between text-center relative bg-white">
                  <div className="text-[7px] font-bold text-zinc-300 border-b border-zinc-50 pb-2 w-full uppercase tracking-widest">{card.suit}</div>
                  <div className="flex-1 flex flex-col items-center justify-center py-2">
                    <div className="transform scale-[0.65] opacity-80">
                      <EssentialIcon type={card.suit === "Major" ? "Major" : card.suit} />
                    </div>
                    <h3 className="text-[10px] font-light italic leading-tight text-zinc-900 px-1 mt-2">{card.name}</h3>
                  </div>
                  <div className="absolute inset-[3px] border border-zinc-50 rounded-[10px] pointer-events-none opacity-30" />
                </div>
              </div>
            ))}
          </div>
        )}

        {/* Interpretation Section */}
        {(isLoading || reading || error) && (
          <section id="reading-display" className="bg-white rounded-[2.5rem] p-8 mb-16 shadow-2xl text-left animate-fade-in relative border border-zinc-100 overflow-visible">
            <div className="flex items-center gap-5 mb-12 border-b border-zinc-50 pb-10">
              <div className="p-4 bg-amber-50 rounded-[2rem] text-amber-500 shadow-inner"><Sparkles size={28} /></div>
              <div>
                <h2 className="text-3xl font-light tracking-tight text-zinc-900 leading-tight">Interpretation</h2>
                <p className="text-[10px] uppercase font-bold text-zinc-400 mt-2 font-sans tracking-[0.3em]">Insights Revealed</p>
              </div>
              {isLoading && <RefreshCw size={24} className="animate-spin text-zinc-200 ml-auto" />}
            </div>
            
            {isLoading ? (
              <div className="space-y-6 py-6">
                {[1, 2, 3].map(i => <div key={i} className="h-4 bg-zinc-50 rounded-full animate-pulse" style={{ width: `${Math.random() * 20 + 80}%` }} />)}
              </div>
            ) : error ? (
              <div className="py-12 text-center text-red-400 text-xs uppercase tracking-widest font-sans font-bold">{error}</div>
            ) : (
              <div className="space-y-10 text-zinc-800 text-xl font-light leading-relaxed first-letter:text-6xl first-letter:float-left first-letter:mr-4 first-letter:text-amber-500 first-letter:font-extralight first-letter:leading-none">
                {reading.split('\n').map((para, i) => para.trim() ? <p key={i}>{para}</p> : null)}
              </div>
            )}
            
            {!isLoading && !error && (
              <div className="mt-20 pt-8 border-t border-zinc-50 flex flex-col items-center gap-6">
                <div className="flex items-center gap-3">
                  <div className="w-1.5 h-1.5 rounded-full bg-amber-400 shadow-md shadow-amber-400/50" />
                  <span className="text-[10px] uppercase tracking-[0.3em] text-zinc-400 font-bold font-sans">Querent Signature: {userZodiac}</span>
                </div>
                <div className="text-zinc-300 italic text-[11px] text-center px-6 leading-relaxed uppercase tracking-[0.2em] font-medium">{activeTagline}</div>
              </div>
            )}
          </section>
        )}

        {/* Modal Picker */}
        {isPickerOpen && (
          <div className="fixed inset-0 z-[100] flex items-center justify-center p-4 bg-zinc-900/10 backdrop-blur-2xl animate-fade-in touch-none">
            <div className="bg-[#fcfcf9] border border-white w-full max-w-lg h-[88vh] rounded-[3rem] overflow-hidden flex flex-col shadow-2xl touch-auto">
              <div className="p-8 border-b border-zinc-100 flex justify-between items-center bg-white/50 shrink-0">
                <div>
                  <h3 className="text-2xl font-light uppercase tracking-tight text-zinc-900 leading-none">The Arcanum</h3>
                  <p className="text-[9px] uppercase font-sans font-bold text-zinc-400 tracking-widest mt-2">Select for Mirror Slot {activePickerSlot + 1}</p>
                </div>
                <button onClick={() => setIsPickerOpen(false)} className="text-zinc-300 hover:text-zinc-900 p-3 transition-colors"><X size={36}/></button>
              </div>
              
              <div className="px-8 py-5 bg-white border-b border-zinc-100 flex items-center shrink-0">
                <Search size={22} className="text-zinc-200 mr-5" />
                <input 
                  type="text" 
                  placeholder="Filter cards..." 
                  className="w-full py-2 text-xl outline-none font-sans placeholder:text-zinc-200 bg-transparent" 
                  value={searchQuery} 
                  onChange={(e) => setSearchQuery(e.target.value)} 
                />
              </div>

              <div className="flex-1 overflow-y-auto p-6 bg-zinc-50/20 momentum-scroll">
                {Object.entries(groupedDeck).map(([suitName, cards]) => (
                  cards.length > 0 && (
                    <div key={suitName} className="mb-12 last:mb-0 px-2 text-left">
                      <div className="flex items-center gap-4 mb-6">
                        <span className="text-[11px] uppercase font-bold tracking-[0.4em] text-amber-600 font-sans">{suitName}</span>
                        <div className="flex-1 h-[1px] bg-amber-100" />
                      </div>
                      <div className="grid grid-cols-2 gap-4">
                        {cards.map(card => {
                          const isTaken = manualSelection.some(c => c?.name === card.name);
                          return (
                            <button 
                              key={card.name} 
                              disabled={isTaken}
                              onClick={() => selectCardFromLibrary(card)} 
                              className={`aspect-[4/5] p-5 rounded-3xl border transition-all relative overflow-hidden flex flex-col justify-between group ${isTaken ? 'bg-zinc-100/50 border-zinc-100 cursor-not-allowed opacity-30 grayscale shadow-none' : 'bg-white border-zinc-100 active:border-amber-400 shadow-sm active:scale-95'}`}
                            >
                              <div className="flex justify-between items-start opacity-40 group-hover:opacity-100 transition-opacity transform scale-[0.4] origin-top-left">
                                <EssentialIcon type={card.suit === "Major" ? "Major" : card.suit} />
                              </div>
                              <div className={`text-sm font-medium leading-tight font-sans ${isTaken ? 'text-zinc-400' : 'text-zinc-800'}`}>{card.name}</div>
                              {isTaken && <div className="absolute inset-0 bg-white/20" />}
                            </button>
                          );
                        })}
                      </div>
                    </div>
                  )
                ))}
              </div>

              <div className="p-10 border-t border-zinc-100 flex justify-end bg-white/80 backdrop-blur-md shrink-0">
                <button onClick={() => setIsPickerOpen(false)} className="px-14 py-4 bg-zinc-900 text-white rounded-full font-bold uppercase text-[10px] tracking-widest shadow-xl font-sans">Return</button>
              </div>
            </div>
          </div>
        )}
      </main>

      <style dangerouslySetInnerHTML={{ __html: `
        @import url('https://fonts.googleapis.com/css2?family=Cormorant+Garamond:ital,wght@0,300;0,400;0,600;1,300;1,400&family=Montserrat:wght@400;700&display=swap');
        
        body { 
          font-family: 'Cormorant Garamond', serif; 
          margin: 0; 
          background-color: #fcfcf7;
          overflow-x: hidden;
        }

        .font-sans { font-family: 'Montserrat', sans-serif; }

        @keyframes fadeIn { from { opacity: 0; transform: translateY(15px); } to { opacity: 1; transform: translateY(0); } }
        .animate-fade-in { animation: fadeIn 0.5s cubic-bezier(0.19, 1, 0.22, 1) forwards; }

        @media (max-width: 400px) {
          .iPhoneTitle { font-size: 1.85rem !important; letter-spacing: 0.35em !important; line-height: 1 !important; }
        }
        
        .momentum-scroll {
          -webkit-overflow-scrolling: touch;
          overflow-y: scroll;
        }

        ::-webkit-scrollbar { width: 4px; }
        ::-webkit-scrollbar-track { background: transparent; }
        ::-webkit-scrollbar-thumb { background: #d4af3744; border-radius: 10px; }
      `}} />
    </div>
  );
};

export default App;
